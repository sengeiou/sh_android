<?xml version="1.0" encoding="UTF-8"?><AccessPolicy Version="1.0.0" Description="Políticas de acceso a las entidades de la aplicación Shooter">    <Entities Name="Login">        <Entity AllowedOperations="Retrieve" isVirtual="true" Hooks="UserLogin" OnlyByKey="true" OnlySpecifiedFilters="false" MaxPageSize="1">            <Name Alias="Login">Login</Name>            <Data>                <!-- USER -->                <Attribute Alias="idUser" Direction="Output" Description="Identificador numérico único de usuario.">idUser</Attribute>                <Attribute Alias="idFavouriteTeam" Direction="Output" Description="Identificador numérico de equipo favorito del usuario.">idFavouriteTeam</Attribute>                <Attribute Alias="sessionToken" Direction="Output" Description="Token de sesión, se usa como password para autenticar las peticiones.">sessionToken</Attribute>                <Attribute Alias="userName" Direction="Output" Description="Nombre de usuario único.">userName</Attribute>                <Attribute Alias="email" Direction="Output" Description="Dirección de email única.">email</Attribute>                <Attribute Alias="name" Direction="Output" Description="Nombre público del usuario, puede repetirse.">name</Attribute>                <Attribute Alias="photo" Direction="Output" Description="URL de imagen de perfil.">photo</Attribute>                <!-- Información de auditoría -->                <Attribute Alias="revision" Direction="Both" Description="Número de revisión">csys_revision</Attribute>                <Attribute Alias="birth" Direction="Both" Description="Fecha de creación">csys_birth</Attribute>                <Attribute Alias="modified" Direction="Both" Description="Fecha de modificación">csys_modified</Attribute>                <Attribute Alias="deleted" Direction="Both" Description="Fecha de borrado">csys_deleted</Attribute>            </Data>            <Keys>                <Key>                    <Attribute Alias="userName">userName</Attribute>                    <Attribute Alias="password">password</Attribute>                </Key>                <Key>                    <Attribute Alias="email">email</Attribute>                    <Attribute Alias="password">password</Attribute>                </Key>            </Keys>        </Entity>    </Entities></AccessPolicy>