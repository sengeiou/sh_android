<?xml version="1.0" encoding="UTF-8"?><AccessPolicy Version="1.0.0" Description="Shooter application entity access policies">    <Entities Name="Login">        <Entity AllowedOperations="Retrieve" isVirtual="true" Hooks="UserLogin" OnlyByKey="true" OnlySpecifiedFilters="false" MaxPageSize="1">            <Name Alias="Login">Login</Name>            <Data>                <!-- USER -->                <Attribute Alias="idUser" Direction="Output" Description="User unique numeric identifier.">idUser</Attribute>                <Attribute Alias="idFavouriteTeam" Direction="Output" Description="Identificador numérico de equipo favorito del usuario.">idFavouriteTeam</Attribute>                <Attribute Alias="sessionToken" Direction="Output" Description="Session token, It is used as password for authentication requests.">sessionToken</Attribute>                <Attribute Alias="userName" Direction="Output" Description="Unique user name.">userName</Attribute>                <Attribute Alias="email" Direction="Output" Description=" Unique mail user.">email</Attribute>                <Attribute Alias="name" Direction="Output" Description=" Public user name, It could be repeated.">name</Attribute>                <Attribute Alias="photo" Direction="Output" Description=" Profile Image url.">photo</Attribute>                <!-- Información de auditoría -->                <Attribute Alias="revision" Direction="Output" Description="It's initialized by 0 to the creation. Must be increased by 1 in each modification ">csys_revision</Attribute>                <Attribute Alias="birth" Direction="Output" Description="It's initialized by current_time to the creation. Only should be modified in the return backend updates">csys_birth</Attribute>                <Attribute Alias="modified" Direction="Output" Description="It's initialized by current_time to the creation. Must be updated at each modification or in the return backend updates">csys_modified</Attribute>                <Attribute Alias="deleted" Direction="Output" Description="It's initialized by null to the creation. Must be updated at current_time when is marked as deleted when is not yet synchronized">csys_deleted</Attribute>            </Data>            <Keys>                <Key>                    <Attribute Alias="userName">userName</Attribute>                    <Attribute Alias="password">password</Attribute>                </Key>                <Key>                    <Attribute Alias="email">email</Attribute>                    <Attribute Alias="password">password</Attribute>                </Key>            </Keys>        </Entity>    </Entities></AccessPolicy>