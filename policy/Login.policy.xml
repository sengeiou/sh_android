<?xml version="1.0" encoding="UTF-8"?><AccessPolicy Version="1.0.0" Description="Políticas de acceso a las entidades de la aplicación Shooter">	<Entities Name="Login">        <Entity AllowedOperations="Retrieve" isVirtual="true" Hooks="UserLogin" OnlyByKey="false" OnlySpecifiedFilters="false" MaxPageSize="1">			<Name Alias="Login">Login</Name>			<Data>				<!-- USER -->                <Attribute Alias="idUser" Direction="Output">idUser</Attribute>                <Attribute Alias="idFavouriteTeam" Direction="Output">idFavouriteTeam</Attribute>                <Attribute Alias="sessionToken" Direction="Output">sessionToken</Attribute>                <Attribute Alias="userName" Direction="Output">userName</Attribute>                <Attribute Alias="email" Direction="Output">email</Attribute>                <Attribute Alias="name" Direction="Output">name</Attribute>                <Attribute Alias="photo" Direction="Output">photo</Attribute>                				<!-- Información de auditoría -->				<Attribute Alias="revision" Direction="Both">csys_revision</Attribute>				<Attribute Alias="birth" Direction="Both">csys_birth</Attribute>				<Attribute Alias="modified" Direction="Both">csys_modified</Attribute>				<Attribute Alias="deleted" Direction="Both">csys_deleted</Attribute>			</Data>			<Keys>				<Key>					<Attribute Alias="userName">userName</Attribute>					<Attribute Alias="password">password</Attribute>				</Key>				<Key>					<Attribute Alias="email">email</Attribute>					<Attribute Alias="password">password</Attribute>				</Key>			</Keys>		</Entity>	</Entities></AccessPolicy>