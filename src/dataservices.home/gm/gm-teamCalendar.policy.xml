<?xml version="1.0" encoding="UTF-8"?><AccessPolicy Version="1.0.0" Description="Políticas de acceso a las entidades de la aplicación Golesmessenger">	<Entities Name="Vistas">		<Entity AllowedOperations="Retrieve" OnlyByKey="false" OnlySpecifiedFilters="true" MaxPageSize="5000">			<Name Alias="TeamCalendar">TeamCalendarView</Name>			<Data>				<!-- Team -->				<Attribute Alias="idTeamLocal" Direction="Output">idTeamLocal</Attribute>				<Attribute Alias="idTeamVisitor" Direction="Output">idTeamVisitor</Attribute>				<Attribute Alias="localNameShort" Direction="Output">localNameShort</Attribute>				<Attribute Alias="visitorNameShort" Direction="Output">visitorNameShort</Attribute>								<!-- Round -->				<Attribute Alias="idRound" Direction="Output">idRound</Attribute>				<Attribute Alias="roundDescription" Direction="Output">roundDescription</Attribute>								<!-- Match/Fixture -->				<Attribute Alias="idMatch" Direction="Output">idMatch</Attribute>				<Attribute Alias="dateMatch" Direction="Output">dateMatch</Attribute>				<Attribute Alias="dateMatchNotConfirmed" Direction="Output">dateMatchNotConfirmed</Attribute>				<Attribute Alias="listTV" Direction="Output">listTV</Attribute>				<Attribute Alias="scoreLocal" Direction="Output">scoreLocal</Attribute>				<Attribute Alias="scoreVisitor" Direction="Output">scoreVisitor</Attribute>				<Attribute Alias="matchState" Direction="Output">matchState</Attribute>                                <!-- Tournament -->				<Attribute Alias="idTournament" Direction="Output">idTournament</Attribute>				<Attribute Alias="tournamentName" Direction="Output">tournamentName</Attribute>				                <!-- Mode -->				<Attribute Alias="idMode" Direction="Output">idMode</Attribute>				<Attribute Alias="modeName" Direction="Output">modeName</Attribute>								<!-- Información de auditoría -->								<Attribute Alias="revision" Direction="Output">csys_revision</Attribute>				<Attribute Alias="birth" Direction="Output">csys_birth</Attribute>				<Attribute Alias="modified" Direction="Output">csys_modified</Attribute>				<Attribute Alias="deleted" Direction="Output">csys_deleted</Attribute>			</Data>			<Keys>				<Key>					<Attribute Alias="idMatch">idMatch</Attribute>				</Key>			</Keys>			<Filters>				<Filter>					<Attribute Alias="idTeamLocal">idTeamLocal</Attribute>					<Attribute Alias="idTeamVisitor">idTeamVisitor</Attribute>					<Attribute Alias="dateMatch">dateMatch</Attribute>					<Attribute Alias="idMode">idMode</Attribute>				</Filter>			</Filters>			<Ordination>				<Attribute Alias="dateMatch" Order="Ascending"/>				<Attribute Alias="idRound" Order="Ascending"/>				<Attribute Alias="idTournament" Order="Ascending"/>				<Attribute Alias="idMode" Order="Ascending"/>			</Ordination>		</Entity>	</Entities></AccessPolicy>