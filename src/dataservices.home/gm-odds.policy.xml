<?xml version="1.0" encoding="UTF-8"?>
<AccessPolicy Version="1.0.0" Description="Políticas de acceso a las entidades de la aplicación Golesmessenger">
	<Entities Name="Cuotas">
		<Entity AllowedOperations="Retrieve" OnlyByKey="false" OnlySpecifiedFilters="true" MaxPageSize="1000">
			<Name Alias="MatchBetType">MatchBetType</Name>
			<Data>
				<Attribute Alias="idMatchBetType" Direction="Output">idMatchBetType</Attribute>
				<Attribute Alias="idProvider" Direction="Output">idProvider</Attribute>
				<Attribute Alias="idMatch" Direction="Output">idMatch</Attribute>
				<Attribute Alias="idBetType" Direction="Output">idBetType</Attribute>
				<!-- Información de auditoría -->				
				<Attribute Alias="revision" Direction="Output">csys_revision</Attribute>
				<Attribute Alias="birth" Direction="Output">csys_birth</Attribute>
				<Attribute Alias="modified" Direction="Output">csys_modified</Attribute>
				<Attribute Alias="deleted" Direction="Output">csys_deleted</Attribute>
			</Data>
			<Keys>
				<Key>
					<Attribute Alias="idMatchBetType">idMatchBetType</Attribute>
				</Key>
				<Key>
					<Attribute Alias="idProvider">idProvider</Attribute>
					<Attribute Alias="idMatch">idMatch</Attribute>
					<Attribute Alias="idBetType">idBetType</Attribute>
				</Key>
			</Keys>
			<Filters>
				<Filter>
					<Attribute Alias="idMatch">idMatch</Attribute>
				</Filter>
				<Filter>
					<Attribute Alias="idBetType">idBetType</Attribute>
				</Filter>
			</Filters>
		</Entity>
		<Entity AllowedOperations="Retrieve" OnlyByKey="true" OnlySpecifiedFilters="true" MaxPageSize="1000">
			<Name Alias="BetType">BetType</Name>
			<Data>
				<Attribute Alias="idBetType" Direction="Output">idBetType</Attribute>
				<Attribute Alias="uniqueKey" Direction="Output">uniqueKey</Attribute>
				<Attribute Alias="name" Direction="Output">name</Attribute>
				<Attribute Alias="comment" Direction="Output">description</Attribute>
				<Attribute Alias="alwaysVisible" Direction="Output">alwaysVisible</Attribute>
				<Attribute Alias="weight" Direction="Output">weight</Attribute>
				<Attribute Alias="title" Direction="Output">title</Attribute>
				<!-- Información de auditoría -->				
				<Attribute Alias="revision" Direction="Output">csys_revision</Attribute>
				<Attribute Alias="birth" Direction="Output">csys_birth</Attribute>
				<Attribute Alias="modified" Direction="Output">csys_modified</Attribute>
				<Attribute Alias="deleted" Direction="Output">csys_deleted</Attribute>
			</Data>
			<Keys>
				<Key>
					<Attribute Alias="idBetType">idBetType</Attribute>
				</Key>
				<Key>
					<Attribute Alias="uniqueKey">uniqueKey</Attribute>
				</Key>
			</Keys>
		</Entity>
		<Entity AllowedOperations="Retrieve" OnlyByKey="false" OnlySpecifiedFilters="true" MaxPageSize="1000">
			<Name Alias="BetTypeOdd">BetTypeOdd</Name>
			<Data>
				<Attribute Alias="idBetTypeOdd" Direction="Output">idBetTypeOdd</Attribute>
				<Attribute Alias="idMatchBetType" Direction="Output">idMatchBetType</Attribute>
				<Attribute Alias="name" Direction="Output">name</Attribute>
				<Attribute Alias="comment" Direction="Output">description</Attribute>
				<Attribute Alias="value" Direction="Output">oddValue</Attribute>
				<Attribute Alias="url" Direction="Output">url</Attribute>
				<!-- Información de auditoría -->				
				<Attribute Alias="revision" Direction="Output">csys_revision</Attribute>
				<Attribute Alias="birth" Direction="Output">csys_birth</Attribute>
				<Attribute Alias="modified" Direction="Output">csys_modified</Attribute>
				<Attribute Alias="deleted" Direction="Output">csys_deleted</Attribute>
			</Data>
			<Keys>
				<Key>
					<Attribute Alias="idBetTypeOdd">idBetTypeOdd</Attribute>
				</Key>
			</Keys>
			<Filters>
				<Filter>
					<Attribute Alias="idMatchBetType">idMatchBetType</Attribute>
				</Filter>
			</Filters>
		</Entity>
		<Entity AllowedOperations="Retrieve" OnlyByKey="false" OnlySpecifiedFilters="false" MaxPageSize="100">
			<Name Alias="Provider">Provider</Name>
			<Data>
				<Attribute Alias="idProvider" Direction="Output">idProvider</Attribute>
				<Attribute Alias="name" Direction="Output">name</Attribute>
				<Attribute Alias="uniqueKey" Direction="Output">uniqueKey</Attribute>
				<Attribute Alias="active" Direction="Output">active</Attribute>
				<Attribute Alias="visible" Direction="Output">visible</Attribute>
				<Attribute Alias="weight" Direction="Output">weight</Attribute>
				<Attribute Alias="disclaimer" Direction="Output">disclaimer</Attribute>
				<Attribute Alias="comment" Direction="Output">comment</Attribute>
				<Attribute Alias="registryURL" Direction="Output">registryURL</Attribute>
				<!-- Información de auditoría -->				
				<Attribute Alias="revision" Direction="Output">csys_revision</Attribute>
				<Attribute Alias="birth" Direction="Output">csys_birth</Attribute>
				<Attribute Alias="modified" Direction="Output">csys_modified</Attribute>
				<Attribute Alias="deleted" Direction="Output">csys_deleted</Attribute>
			</Data>
			<Keys>
				<Key>
					<Attribute Alias="idProvider">idProvider</Attribute>
				</Key>
				<Key>
					<Attribute Alias="uniqueKey">uniqueKey</Attribute>
				</Key>
			</Keys>
		</Entity>
	</Entities>
</AccessPolicy>
