<?xml version="1.0" encoding="UTF-8"?><AccessPolicy Version="1.0.0" Description="Políticas de acceso a las entidades de la aplicación FCBPenyes">	<Entities Name="Entidades Base">		<Entity AllowedOperations="Retrieve" OnlyByKey="false" OnlySpecifiedFilters="false" MaxPageSize="2000">			<Name Alias="ContentLanguage">ContentLanguage</Name>			<Data>				<!-- CONTENT -->                <Attribute Alias="idContent" Direction="Output">idContent</Attribute>                <Attribute Alias="idLanguage" Direction="Output">idLanguage</Attribute>				<Attribute Alias="title" Direction="Output">title</Attribute>				<Attribute Alias="subtitle" Direction="Output">subtitle</Attribute>				<Attribute Alias="contentText" Direction="Output">contentText</Attribute>				<Attribute Alias="shortTitle" Direction="Output">shortTitle</Attribute>				<Attribute Alias="shortSubtitle" Direction="Output">shortSubtitle</Attribute>				<Attribute Alias="authorship" Direction="Output">authorship</Attribute>                <Attribute Alias="epigraph" Direction="Output">epigraph</Attribute>				<Attribute Alias="publicDate" Direction="Output">publicDate</Attribute>				<Attribute Alias="tags" Direction="Output">tags</Attribute>				<Attribute Alias="url" Direction="Output">url</Attribute>                <Attribute Alias="multimediaEmbed" Direction="Output">multimediaEmbed</Attribute>				<Attribute Alias="breakdowns" Direction="Output">breakdowns</Attribute>				<Attribute Alias="quotes" Direction="Output">quotes</Attribute>				<Attribute Alias="hashValidation" Direction="Output">hashValidation</Attribute>								<!-- Información de auditoría -->								<Attribute Alias="revision" Direction="Output">csys_revision</Attribute>				<Attribute Alias="birth" Direction="Output">csys_birth</Attribute>				<Attribute Alias="modified" Direction="Output">csys_modified</Attribute>				<Attribute Alias="deleted" Direction="Output">csys_deleted</Attribute>			</Data>			<Keys>				<Key>					<Attribute Alias="idContent">idContent</Attribute>                    <Attribute Alias="idLanguage">idLanguage</Attribute>                    <Attribute Alias="originalType">originalType</Attribute>				</Key>			</Keys>		</Entity>        <Entity AllowedOperations="Retrieve" OnlyByKey="false" OnlySpecifiedFilters="false" MaxPageSize="2000">			<Name Alias="Penya">Penya</Name>			<Data>				<!-- CONTENT -->                <Attribute Alias="idPenya" Direction="Output">idPenya</Attribute>                <Attribute Alias="adress" Direction="Output">adress</Attribute>				<Attribute Alias="typeAdress" Direction="Output">typeAdress</Attribute>				<Attribute Alias="numberAdress" Direction="Output">numberAdress</Attribute>				<Attribute Alias="floor" Direction="Output">floor</Attribute>				<Attribute Alias="door" Direction="Output">door</Attribute>				<Attribute Alias="stair" Direction="Output">stair</Attribute>				<Attribute Alias="city" Direction="Output">city</Attribute>                <Attribute Alias="province" Direction="Output">province</Attribute>				<Attribute Alias="mail" Direction="Output">mail</Attribute>				<Attribute Alias="cp" Direction="Output">cp</Attribute>				<Attribute Alias="urlImage" Direction="Output">urlImage</Attribute>                <Attribute Alias="telephone" Direction="Output">telephone</Attribute>				<Attribute Alias="web" Direction="Output">web</Attribute>				<Attribute Alias="lon" Direction="Output">lon</Attribute>				<Attribute Alias="lat" Direction="Output">lat</Attribute>								<!-- Información de auditoría -->				<Attribute Alias="revision" Direction="Output">csys_revision</Attribute>				<Attribute Alias="birth" Direction="Output">csys_birth</Attribute>				<Attribute Alias="modified" Direction="Output">csys_modified</Attribute>				<Attribute Alias="deleted" Direction="Output">csys_deleted</Attribute>			</Data>			<Keys>				<Key>					<Attribute Alias="idPenya">idPenya</Attribute>				</Key>			</Keys>			<Filters>				<Filter>					<Attribute Alias="idPenya">idPenya</Attribute>				</Filter>			</Filters>		</Entity>	</Entities></AccessPolicy>