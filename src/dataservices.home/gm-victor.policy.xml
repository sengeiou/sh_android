<?xml version="1.0" encoding="UTF-8"?><AccessPolicy Version="1.0.0" Description="Políticas de acceso a las entidades de la aplicación Golesmessenger">	<Entities Name="Entidades Base">  		<Entity AllowedOperations="Retrieve" OnlyByKey="false" OnlySpecifiedFilters="false" MaxPageSize="1000">			<Name Alias="Team">Team</Name>			<Data>				<!-- Team -->				<Attribute Alias="idTeam" Direction="Output">idTeam</Attribute>				<Attribute Alias="Name" Direction="Output">Name</Attribute>				<Attribute Alias="NameShort" Direction="Output">NameShort</Attribute>				<Attribute Alias="URLImage" Direction="Output">URLImage</Attribute>				<Attribute Alias="isNationalTeam" Direction="Output">isNationalTeam</Attribute>			</Data>			<Keys>				<Key>					<Attribute Alias="idTeam">idTeam</Attribute>				</Key>			</Keys>		</Entity>	</Entities></AccessPolicy>