<?xml version="1.0" encoding="UTF-8"?><AccessPolicy Version="1.0.0" Description="Políticas de acceso a las entidades de la aplicación Golesmessenger">	<Entities Name="Suscripciones">		<Entity AllowedOperations="Retrieve" OnlyByKey="false" OnlySpecifiedFilters="true" MaxPageSize="1000">			<Name Alias="Subscription">Subscription</Name>			<Data>				<!-- Subscription -->				<Attribute Alias="idDevice" Direction="Output">idDevice</Attribute>				<Attribute Alias="idMatch" Direction="Output">idMatch</Attribute>				<Attribute Alias="idTeam" Direction="Output">idTeam</Attribute>				<Attribute Alias="idSubscription" Direction="Output">idSettingSubscription</Attribute>				<Attribute Alias="idAllEvents" Direction="Output">idAllEvents</Attribute>				<Attribute Alias="idSML" Direction="Output">idSML</Attribute>				<Attribute Alias="negation" Direction="Output">negation</Attribute>												<!-- Información de auditoría -->								<Attribute Alias="revision" Direction="Output">csys_revision</Attribute>				<Attribute Alias="birth" Direction="Output">DateIn</Attribute>				<Attribute Alias="modified" Direction="Output">LastUpdate</Attribute>				<Attribute Alias="deleted" Direction="Output">csys_deleted</Attribute>			</Data>			<Keys>				<Key>					<Attribute Alias="idSubscription">idSettingSubscription</Attribute>				</Key>			</Keys>			<Filters>				<Filter>					<Attribute Alias="idDevice">idDevice</Attribute>				</Filter>				<Filter>                    <Attribute Alias="idDevice">idDevice</Attribute>					<Attribute Alias="idTeam">idTeam</Attribute>				</Filter>				<Filter>                    <Attribute Alias="idDevice">idDevice</Attribute>					<Attribute Alias="idMatch">idMatch</Attribute>				</Filter>			</Filters>		</Entity>	</Entities></AccessPolicy>