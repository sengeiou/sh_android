<?xml version="1.0" encoding="UTF-8"?><AccessPolicy Version="1.0.0" Description="Políticas de acceso a las entidades de la aplicación Golesmessenger">	<Entities Name="Entidades Base">		<Entity AllowedOperations="Retrieve" OnlyByKey="false" OnlySpecifiedFilters="true" MaxPageSize="100">			<Name Alias="SML">N_SOUND_MESSAGE_LANGUAGE</Name>			<Data>				<!-- SML -->				<Attribute Alias="idSML" Direction="Output">idSML</Attribute>				<Attribute Alias="sound" Direction="Output">sound</Attribute>				<Attribute Alias="message" Direction="Output">message</Attribute>				<Attribute Alias="language" Direction="Output">language</Attribute>								<!-- Información de auditoría -->								<Attribute Alias="revision" Direction="Output">csys_revision</Attribute>				<Attribute Alias="birth" Direction="Output">csys_birth</Attribute>				<Attribute Alias="modified" Direction="Output">csys_modified</Attribute>				<Attribute Alias="deleted" Direction="Output">csys_deleted</Attribute>			</Data>			<Keys>                <Key>                    <Attribute Alias="idSML">idSML</Attribute>                </Key>			</Keys>			<Filters>				<Filter>                    <Attribute Alias="idSML">idSML</Attribute>                </Filter>			</Filters>		</Entity>	</Entities></AccessPolicy>